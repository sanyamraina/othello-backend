# Supabase Configuration
# Copy this file to .env and fill in your actual Supabase credentials

# Your Supabase project URL
# Get this from your Supabase project dashboard
SUPABASE_URL=https://your-project-id.supabase.co

# Your Supabase anon/public key  
# Get this from Project Settings > API in your Supabase dashboard
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Enable debug logging for development
# LOG_LEVEL=DEBUG

# Optional: For testing (do not use in production)
# TEST_MODE=false
# CACHE_TABLE_NAME=positions